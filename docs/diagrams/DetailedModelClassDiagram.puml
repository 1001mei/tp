@startuml
!include style.puml

$OverrideTheme(MODEL_COLOR)

package Model {
    class Product
    class Name
    class Category
    class Description
    class Price

    class UniqueItemList
    class Item
    class ItemDescriptor
    class ExpiryDate
    class Quantity
}

Product *---> Name
Product *---> Category
Product *---> Description
Product *---> Price
Product *--> UniqueItemList

UniqueItemList --> "*" Item

Item *--> "1" Product
Item --|> ItemDescriptor

ItemDescriptor *--> ExpiryDate
ItemDescriptor *--> Quantity

@enduml

@startuml
!include style.puml

'Local overrides (copy this section)
'Change the color here
!$theme_color = LOGIC_COLOR

skinparam TitleFontColor $theme_color
skinparam Class {
    FontColor $theme_color
    StereotypeFontColor $theme_color
    BorderColor $theme_color
    FontColor<<Hidden>> #FFFFFF
    StereotypeFontColor<<Hidden>> #FFFFFF
    BorderColor<<Hidden>> #FFFFFF
}
'End of local overrides

class "{abstract}\nCommand" as Command
class XYZCommand

package "Parser classes" {
    class "<<interface>>\nParser" as Parser
    class AddressBookParser
    class XYZCommandParser
    class CliSyntax
    class ParserUtil
    class ArgumentMultimap
    class ArgumentTokenizer
    class Prefix
}

class HiddenOutside<<Hidden>>
HiddenOutside ..> AddressBookParser

AddressBookParser .down.> XYZCommandParser: creates >

XYZCommandParser ..> XYZCommand : creates >
AddressBookParser ..> Command : returns >
XYZCommandParser .up.|> Parser
XYZCommandParser ..> ArgumentMultimap
XYZCommandParser ..> ArgumentTokenizer
ArgumentTokenizer .left.> ArgumentMultimap
XYZCommandParser ..> CliSyntax
CliSyntax ..> Prefix
XYZCommandParser ..> ParserUtil
ParserUtil .down.> Prefix
ArgumentTokenizer .down.> Prefix
XYZCommand -up-|> Command
@enduml
